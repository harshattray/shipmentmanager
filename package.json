{
  "name": "shipmentmanager",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^3.19.3",
    "axios": "^0.18.0",
    "axios-mock-adapter": "^1.16.0",
    "babel-polyfill": "^6.26.0",
    "fetch-mock": "^7.3.3",
    "fuse.js": "^3.4.5",
    "json-server": "^0.15.0",
    "moment": "^2.24.0",
    "moxios": "^0.4.0",
    "react": "^16.8.6",
    "react-bootstrap-typeahead": "^4.0.0-alpha.10",
    "react-dom": "^16.8.6",
    "react-paginate": "^6.3.0",
    "react-redux": "^7.0.2",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "react-scripts": "3.0.0",
    "react-select": "^2.4.3",
    "redux": "^4.0.1",
    "redux-form": "^8.2.0",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.86.0",
    "styled-components": "^4.2.0"
  },
  "scripts": {
    "start": "json-server --watch db.js --port 3003 & webpack-dev-server --open --port 3001 --config webpack.dev.js",
    "clean": "rm -rf deploy",
    "prod": "npm install --dev && npm run build",
    "build": "webpack --config webpack.prod.js && cp _redirects dist/",
    "commit_deploy": "git add deploy -A && git commit -m \"deploy files at `date`\" -n > /dev/null 2>&1; exit 0",
    "ghpages": "git subtree split --prefix deploy/ -b gh-pages && git push -f origin gh-pages:gh-pages && git branch -D gh-pages",
    "deploy": "npm run build && npm run commit_deploy && npm run ghpages",
    "test": "jest --coverage",
    "test:watch": "NODE_ENV=test jest --watchAll"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-stage-0": "^7.0.0",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.0-beta.6",
    "clean-webpack-plugin": "^2.0.1",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^1.0.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "enzyme-to-json": "^3.3.4",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "postcss-loader": "^3.0.0",
    "react-test-renderer": "^16.8.6",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.1.4"
  }
}
